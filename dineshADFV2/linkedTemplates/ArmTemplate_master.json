{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "dineshADFV2"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorageTgt_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageTgt'"
		},
		"AzureBlob1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "transactions.txt"
		},
		"AzureBlob1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "tutorial/Source"
		},
		"AzureBlob2tgt_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"AzureBlob2tgt_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "tutorial"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorageTgt_connectionString": {
						"value": "[parameters('AzureBlobStorageTgt_connectionString')]"
					},
					"AzureBlob1_properties_typeProperties_fileName": {
						"value": "[parameters('AzureBlob1_properties_typeProperties_fileName')]"
					},
					"AzureBlob1_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureBlob1_properties_typeProperties_folderPath')]"
					},
					"AzureBlob2tgt_properties_typeProperties_fileName": {
						"value": "[parameters('AzureBlob2tgt_properties_typeProperties_fileName')]"
					},
					"AzureBlob2tgt_properties_typeProperties_folderPath": {
						"value": "[parameters('AzureBlob2tgt_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}